<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:colorPrimary">@color/app_bar_fondo</item>
        <item name="android:colorPrimaryDark">@color/app_bar_dark</item>
        <item name="colorAccent">@color/accent</item>
        <item name="toolbarStyle">@style/Widget.AppCompat.Toolbar</item>
        <item name="toolbarNavigationButtonStyle">@style/buttonToolbar</item>
    </style>
    <style name="estilo_preguntas">
        <item name="android:textColor">@color/preguntasColor</item>
    </style>
    <style name="buttonToolbar" parent="@style/Widget.AppCompat.Toolbar.Button.Navigation">
        <item name="android:minWidth">0dp</item>
        <item name="android:padding">12dp</item>
        <item name="android:scaleType">centerInside</item>
    </style>
    <style name="estilo_preguntas_actividad4">
        <item name="android:textColor">@color/preguntasColor</item>
    </style>
    <style name="estilo_fab">
        <item name="android:layout_marginBottom">10dp</item>
    </style>
    <style name="estilo_fab_560">
        <item name="android:layout_marginBottom">7dp</item>
    </style>
    <style name="webview_estilo_560">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">240dp</item>
    </style>
    <style name="estilo_fab_Xhdpi">
        <item name="android:layout_marginBottom">8dp</item>
    </style>
    <style name="estilo_fab_Xxhdpi">
        <item name="android:layout_marginBottom">28dp</item>
    </style>
</resources>